@model WebMvc.ViewModels.ChartViewModel

<canvas id="@Model.Id" class="@Model.CssClass"></canvas>
<script>
        $(function () {
            const dataset = [{
                label: "@Model.Description",
                backgroundColor: 'rgb(0,128,0)',
                borderColor: 'rgb(0,128,0)',
                data: @Json.Serialize(Model.Data),
            }];

            ChartUtil.CreateChart("@Model.Id", '@Model.ChartType', dataset);
        });
</script>